{
  "expo": {
    "name": "Aurashop",
    "slug": "aurashop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Lineman.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "bundleIdentifier": "com.itgenius.aurashop",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let you upload profile photos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you upload profile photos."
      }
    },
    "android": {
      "package": "com.itgenius.aurashop",
      "versionCode": 1,
      "ndkVersion": "26.0.10792818",
      "adaptiveIcon": {
        "foregroundImage": "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTwWw5k7-OeG8hrlKAYrVcfX4aTMxOe0KFCeA&s",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Lineman.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share photos with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2f64323a-5e0e-4775-ade6-c83905188b74"
      }
    },
    "owner": "mongkol251"
  }
}
